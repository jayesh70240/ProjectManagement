<% if user_signed_in? %>
    <%= button_to "Log Out",destroy_user_session_path,method: :delete,class:"btn btn-secondary"%>
    <%= link_to "Edit",edit_user_registration_path,class:"btn btn-secondary"%>

<% else%>
    <%= button_to "Log in",new_user_session_path,class:"btn btn-primary"%>
    <%= link_to "Registration",new_user_registration_path,class:"btn btn-primary"%>
<% end %>